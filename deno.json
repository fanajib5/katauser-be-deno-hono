{
  "imports": {
    "hono": "jsr:@hono/hono@^4.8.12",
    "@supabase/supabase-js": "https://esm.sh/@supabase/supabase-js@2"
  },
  "tasks": {
    "dev": "deno run --allow-env --allow-net --watch dev.ts",
    "migrate": "deno run --env=.env --allow-env --allow-read scripts/migrate.ts",
    "start": "deno run --allow-env --allow-net main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
