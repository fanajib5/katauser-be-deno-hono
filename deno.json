{
  "imports": {
    "hono": "jsr:@hono/hono@^4.8.12"
  },
  "tasks": {
    "dev": "deno run --env=.env --allow-env --allow-net --watch dev.ts",
    "start": "deno run --allow-env --allow-net main.ts",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:migrate": "npx drizzle-kit push"
  },
  "fmt": {
    "options": {
      "semiColons": true
    }
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
